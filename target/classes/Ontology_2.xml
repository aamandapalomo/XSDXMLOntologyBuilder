        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/implemented" vs:term_status="testing"
                  rdfs:label="implemented"
                  rdfs:comment="implemented">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/ImplementationRequirement"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Content"/>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/prohibited_implementation" vs:term_status="testing"
